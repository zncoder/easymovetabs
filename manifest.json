{
  "name": "easymovetabs",
  "version": "1.7.0",
  "manifest_version": 3,
  "description": "Move highlighted tabs to another window",
	"permissions": [
		"tabs"
	],
	"icons": {
		"48": "48.png"
	},
	"background": {
		"service_worker": "background.js"
	},
	"action": {
		"browser_style": true,
		"default_icon": "32.png",
		"default_title": "Select tabs to move"
	}
}
