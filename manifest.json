{
	"name": "easymovetabs",
	"version": "1.8.1",
	"manifest_version": 3,
	"description": "Move highlighted tabs to another window",
	"permissions": [
		"contextMenus",
		"tabs"
	],
	"icons": {
		"48": "48.png"
	},
	"background": {
		"service_worker": "background.js"
	},
	"action": {
		"browser_style": true,
		"default_icon": "32.png",
		"default_title": "Select tabs to move"
	}
}
